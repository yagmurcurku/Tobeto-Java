
public class Main {

	public static void main(String[] args) {
		int toplam = 0;
		// total --> 10 tane sayı yazdırıldığında program duracak
		int total = 0;
		for(int i = 1; total == 10; i++) {
			
			
			for(int y = 1; y <= i ; y++) {
				if(i % y == 0) {
					toplam += y;
				}
			}
			if(toplam == (i*2)) {
				total+=1;
			}
			
		}
		System.out.println(total);
		
		
	}
}
