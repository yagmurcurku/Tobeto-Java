package eTicaretDemo.models;

public class Order {

	private int id;
	Product product;
	
	public Order() {
		
	}
	
	public Order(int id, Product product) {
		this.id = id;
		this.product = product;
	}

	
	public int getId() {
		return id;
	}

	public void setId(int id) {
		this.id = id;
	}

	public Product getProduct() {
		return product;
	}

	public void setProduct(Product product) {
		this.product = product;
	}


	
	
	
	
}
